<?xml version="1.0" encoding="UTF-8"?>

<configuration>

	<!-- Accounts for a specific client -->
	
	<!-- To use this test, copy this file to config-test.xml and
		 fill in configuration details.
		 
		 Test details are found here
		 https://www.svea.com/se/sv/foretag/betallosningar/betallosningar-for-e-handel/tech-site/?currentTab=custom-integration&subTab=test-instructions
		 
	 -->
	 
	
	<!--
	
			Possible account types 
			- invoice
			- paymentplan
			- account-credit
			- creditcard
			 
	 -->
	<accounts>
	
		<account>
			<accountNo></accountNo>
			<username></username>
			<password></password>
			<accountType>invoice</accountType>
			<countryCode>SE</countryCode>
		</account>
		
		<account>
			<accountNo></accountNo>
			<username></username>
			<password></password>
			<accountType>paymentplan</accountType>
			<countryCode>SE</countryCode>
		</account>
		
		<account>
			<accountNo></accountNo>
			<username></username>
			<password></password>
			<accountType>account-credit</accountType>
			<countryCode>SE</countryCode>
		</account>
		
		<!-- Credit card account, reconciliation from Payment Gateway -->
		
		<account>
			<merchantId>NNNN</merchantId>
			<accountType>creditcard</accountType>
			<secretWord>xxxxxx0000xxxx</secretWord>
			<server>https://webpay.sveaekonomi.se</server>
		</account>
		
	</accounts>

</configuration>